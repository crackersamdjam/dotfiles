profile {
	output eDP-1 enable scale 1.5
}
profile {
	output 'Dell Inc. DELL P3223QE HKYTXN3' enable scale 2
	output eDP-1 disable
}
profile {
	output "Dell Inc. DELL U2720Q J03YW83" enable scale 2
	output eDP-1 disable
}

